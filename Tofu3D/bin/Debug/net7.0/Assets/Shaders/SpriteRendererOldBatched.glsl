[BUFFERTYPE:Sprite][VERTEX]#version 410 corelayout (location = 0) in vec4 vertexPosition;layout (location = 1) in vec4 aTexCoord;layout(location = 2) in vec2 batching_position;layout(location = 3) in vec2 batching_size;out vec4 texCoord;out vec4 frag_color;uniform mat4 u_mvp = mat4(1.0);void main(void){//texCoord = aTexCoord;//gl_Position = u_mvp * position;texCoord = aTexCoord;    vec2 temp =vec2( vertexPosition.x* batching_size.x ,vertexPosition.y*batching_size.y);    temp = temp + batching_position;    gl_Position =  (u_mvp * vec4(temp.x,temp.y,vertexPosition.z,vertexPosition.w));}[FRAGMENT]#version 410 core in vec4 c;in vec4 texCoord;uniform sampler2D textureObject;uniform vec4 u_color = vec4(1.0);uniform vec2 u_repeats = vec2(1.0);layout (location = 0) out vec4 color;void main(void){vec4 texColor = texture(textureObject, vec2(texCoord.x*u_repeats.x + 0.5, texCoord.y*u_repeats.y + 0.5)) * u_color;if (texColor.a < 0.1){discard;}else{color = texColor;}}